</filter>


####################
# ALL SERVICES
####################
<filter jfrog.**.router.request>
  @type parser
  key_name message
  <parse>
    @type json
    time_key time
    time_format %Y-%m-%dT%H:%M:%S%:z
  </parse>
</filter>

<filter jfrog.**.router.traefik>
  @type parser
  key_name message
  <parse>
